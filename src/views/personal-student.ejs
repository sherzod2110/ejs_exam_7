<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../assets/css/main.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>
<body class="body">
    


    <div class="container-fluid p-0 d-flex">


        <div class="navbarr">
            <div class="d-flex align-items-center justify-content-between border-2 border-bottom mx-auto p-4" style="width: 247px;">
                <img src="../assets/img/user.svg" alt="" width="48" height="48">
                <h2 class="heading">CRM <br> PANEL</h1>
                    <img src="../assets/img/menu.svg" alt="" width="40" height="40">
            </div>

            <ul class="list-unstyled m-0 mt-4">
                <a href="/personal/student" class="text-decoration-none d-flex item align-items-center gap-3" style="padding-left: 28px;">
                    <img src="../assets/img/graduation-cap-solid 1 (1).svg" alt="" width="26" height="26">
                    <p class="m-0">Student</p>
                </a>
            </ul>
            
        </div>


        <div class="w-100">

            <div class="nav p-3 pe-5 d-flex justify-content-between align-items-center">
                <h2 class="head">Student</h2>
                <p class="p-0 time"><%= time %> </p>
                <div class="gap-3 d-flex">
                    <img src="../assets/img/Group 40.svg" alt="">
                    <img src="../assets/img/Group 42.svg" alt="">
                </div>
            </div>

            <div class="box">
                <h1 class="new-team">Student</h1>

                <table class="table table-striped scrol">
                    <thead>
                      <tr>
                        <th>O'quvchi</th>
                        <th>Role</th>
                        <th>Telefon</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% student.forEach(e => { %>
                        <tr>
                          <td><%= e.userName %> </td>
                          <td><%= e.role %> </td>
                          <td><%= e.tel %> </td>
                        </tr>
                      <% }) %>
                    </tbody>
                  </table>

                


                <h1 class="pt-4 new-team">Uyga Vazifa</h1>


                <ul class="list-unstyled d-flex gap-5 mt-4 flex-wrap list-2 pb-2">
                    <% allTask.forEach(e => { %>
                        <li class="item-3 p-3">
                          <p class="text-tech"><span class="spn-rol">Month:</span> <span class="sd"> <%= month %>  <%= monthh %></span> </p>
                          <p class="text-tech"><span class="spn-rol">Day:</span> <span class="sd"> <%= date %> Day</span> </p>
                          <p class="text-tech"><span class="spn-rol">Time:</span> <span class="sd"> <%= time2 %> </span> </p>
                          <p class="text-tech"><span class="spn-rol">Vazifa:</span> <span class="sd"><%= e.hometask %></span> </p>
                        </li>
                      <% }) %>
                </ul>
                

            </div>

        </div>




    </div>


</body>
</html>